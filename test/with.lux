extern "C"
    fn printf(fmt: *Char, ...) -> Int

type Point
    x: Int
    y: Int

fn main() -> Int
    let p = Point(1, 2)
    let p2 = p with {x: 10}
    printf("p = (%d, %d)\n", p.x, p.y)
    printf("p2 = (%d, %d)\n", p2.x, p2.y)

    let p3 = p with {x: 100, y: 200}
    printf("p3 = (%d, %d)\n", p3.x, p3.y)

    return 0
